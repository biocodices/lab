    <%= link_to edit_nanodrop_quantification_path(nanodrop_quantification),
        title: "Edit this table" do %>
      <span class="grey small">edit</span>
    <% end %>
  </th>
  <tr>
    <th>Technology</th>
    <td>Nanodrop</td>
